class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        sum_list = []
        for i in range(len(nums)-1):
            first = nums[i]
            for j in range(i + 1,len(nums)):
                second = nums[j]
                target - first == second
                if second in nums:
                    return [nums.index(first),nums.index(second)]
